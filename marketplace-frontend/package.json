{
  "name": "nft-marketplace-frontend",
  "version": "1.0.0",
  "description": "NFT Marketplace frontend application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -b --noEmit",
    "verify-env": "node scripts/verify-env.js",
    "verify-contracts": "node scripts/verify-contracts.js",
    "verify-all": "npm run verify-env && npm run verify-contracts",
    "test-api": "node scripts/test-api.js",
    "debug-api": "node scripts/debug-api-response.js"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "ethers": "^6.15.0",
    "recharts": "^3.3.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "typescript": "^5.9.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "vite": "^7.1.10",
    "@vitejs/plugin-react": "^4.3.4",
    "tailwindcss": "^4.1.14",
    "@tailwindcss/vite": "^4.1.14"
  }
}